<Window x:Class="DeLauncherForm.Windows.GentoolUpdateFailed"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DeLauncherForm.Windows"
        mc:Ignorable="d"
        Title="GentoolUpdateFailed" Height="330" Width="850" WindowStyle="SingleBorderWindow" ResizeMode="NoResize">
    <Window.Background>
        <ImageBrush ImageSource="Resources/Gentools.png"/>
    </Window.Background>
    <Grid>
        <TextBox x:Name = "ErrorMessage1" IsReadOnly="True" HorizontalAlignment="Left" Height="35" Margin="77,215,0,0" TextWrapping="Wrap" Text="Gentool update failed, resuming launch with current version" FontSize="20" VerticalAlignment="Top" Width="678"
                 Background="LightGray"/>
        <Button x:Name="Ok" Content="OK" IsDefault = "true" HorizontalAlignment="Left" Margin="369,255,0,0" VerticalAlignment="Top" Width="102" Height="37" FontSize="20"/>

    </Grid>
</Window>
